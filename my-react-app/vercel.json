{
  "rewrites": [
    {
      "source": "/api",
      "destination": "https://https://one-fitness-drive-cru6.vercel.app/api"
    },
    {
      "source": "/api/:path*",
      "destination": "https://https://one-fitness-drive-cru6.vercel.api/api/:path*"
    }
  ],
  "routes": [
     { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
